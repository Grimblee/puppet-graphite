[default]
default=true
<% destinations = ''
for d in 1..@carbon_cache_amount.to_i do
  destinations += (@relay_destinations+':'+(@carbon_pickle_receiver_port.to_i + 10*d - 10).to_s+':'+d.to_s)
  if d < @carbon_cache_amount.to_i
    destinations += ', '
  end 
end
-%>
destinations = <%= destinations %>
